{
  "name": "realworld",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "lint-inspector": "npx @eslint/config-inspector@latest",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --run --coverage",
    "e2e:open": "cross-env NODE_ENV=development playwright test --ui",
    "e2e:run": "cross-env NODE_ENV=development playwright test",
    "e2e:run:headed": "cross-env NODE_ENV=development playwright test --headed",
    "e2e:debug": "cross-env NODE_ENV=development playwright test --debug",
    "e2e:report": "playwright show-report",
    "e2e:ci": "playwright test",
    "e2e:ci:preview": "playwright test --grep @preview",
    "prepare": "husky",
    "test:pre-push": "npm test"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^5.2.2",
    "@reduxjs/toolkit": "^2.9.2",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@vanilla-extract/css": "^1.17.4",
    "axios": "^1.12.2",
    "classnames": "^2.5.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.65.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.5",
    "redux-persist": "^6.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.38.0",
    "@playwright/test": "^1.57.0",
    "@stylistic/eslint-plugin": "^3.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.38.0",
    "eslint-config-airbnb-extended": "^2.3.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.2.6",
    "msw": "^2.12.4",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,js,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
